<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65" fx:controller="view.Registration">
   <children>
      <GridPane layoutX="110.0" layoutY="94.0" prefHeight="211.0" prefWidth="304.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="username" />
            <Label text="pass" GridPane.rowIndex="1" />
            <Label text="type" GridPane.rowIndex="2" />
            <TextField fx:id="usernamefild" GridPane.columnIndex="1" />
            <TextField fx:id="passwordfild" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="typefild" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <ButtonBar layoutX="162.0" layoutY="313.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#sign" text="OK" />
